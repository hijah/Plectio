        <ListView x:Name="Fredag_list" Margin="1048,200,0,0" BorderBrush="Black" BorderThickness="1" Foreground="Black" HorizontalAlignment="Left" Width="184" Height="528" VerticalAlignment="Top"/>
        <Button x:Name="Tilbage_knap" Content="Tilbage" Margin="1242,404,0,0" VerticalAlignment="Top" Width="117" FontSize="24" Foreground="Black" BorderBrush="Black" HorizontalAlignment="Left" Height="54">
